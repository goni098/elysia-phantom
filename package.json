    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "randomstring": "^1.3.0",
    "telegraf": "^4.16.3",
    "tweetnacl": "^1.0.3",
    "typegram": "^5.2.0"
  },
  "devDependencies": {
    "bun-types": "latest",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@cspell/eslint-plugin": "^8.13.0",
    "@eslint/js": "^9.8.0",
    "@types/lodash": "^4.17.7",
    "@types/luxon": "^3.4.2",
    "@types/randomstring": "^1.3.0",
    "eslint": "^9.8.0",
