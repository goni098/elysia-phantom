  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "keywords": [],
  "type": "module",
  "scripts": {
    "dev": "bun run --watch --env-file ./.env.local src/bin/server.ts",
    "start": "bun run --env-file ./.env.local src/bin/server.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --cache --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma": "prisma",
    "preinstall": "npx only-allow bun",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "check": "bun type-check && bun lint"
